{{!-- <h1>hey iam card here </h1> --}}
<div class="">
<img  style="position:absolute;top:0em" width="100%"  height="100%" src="https://cms-cdn.lux.camera/wp-content/uploads/2022/11/01170023/top-mid-1424x862.jpg">
</div>

<section >
    <div class="container">
        <div class="row mt-5">
            {{#each products}}
            <div class="col-12 col-sm-6 col-md-3">
                <div class="card" style="width: 14rem;">
                    <img class="card-img-top"
                        src="{{this.image}}"
                        alt="Card image cap">
                    <div class="card-body">
                        <h5 class="card-title">{{this.name}}</h5>
                        <p class="card-text">{{this.description}}</p>
                        <a href="#" class="btn btn-primary">Add to Cart</a>
                    </div>
                  </div>
                </div>
            {{/each}}
            
        </div>
    </div>
</section>