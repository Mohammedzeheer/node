{{!-- <h3>Tables</h3> --}}
<div class="container mt-5">
<table style="position:relative;margin:2em" class="table table-striped table-hover" >
<thead class="table-dark">
  <tr>
    {{#each people.[0]}}
      <th>{{@key}}</th>
    {{/each}}
  </tr>
</thead>
<tbody>
  {{#each people}}
    <tr>
    	{{#each this}}
        <td>{{this}}</td>
      {{/each}}
    </tr>
  {{/each}}
</tbody>
</table>
</div>

